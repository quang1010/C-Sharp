using System;

namespace baitap
{
    class b1
    {
        public static void Main(string[] args)
        {
            int[] array = new int[3];
            array[0] = 2;
            array[1] = 3;
            array[2] = 4;
            int total = 0;
            for(int i = 0; i < array.Length; i++)
            {
                total += array[i];
            }
            Console.Write(total);
        }
    }
    
